<html ng-app="TestApp">
    <head>
        <title>The MEAN Stack App</title>

        <script src="assets/js/angular.min.js">
        </script>
    </head>
    <body ng-controller="MainController as vm">

        <input type="text" ng-model="vm.message" />
        <br />
        {{ vm.message }}
        <br />
        <ul>
            <li ng-repeat="person in vm.people">
                {{ person.name }}
            </li>
        </ul>

        <script src="assets/js/app.js">
        </script>

    </body>
</html>
